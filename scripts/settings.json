{
    "name": "AppSource Base",
    "memoryLimit": "6G",
    "installApps": "output/output/Apps/", <-- wenn BaseApp = Dependency 
    "installApps": "", <-- Pipelines anpassen, wenn BaseApp <> Dependency
    "appFolders": "app",
    "testFolders": "test",
    "installTestRunner": true,
    "installTestFramework": true,
    "installTestLibraries": false,
    "installPerformanceToolkit": false,
    "doNotSignApps": true,
    "enableCodeCop": false,
    "enableAppSourceCop": true,
    "appSourceCopMandatoryAffixes": [
        "TWE"
    ],
    "enablePerTenantExtensionCop": false,
    "enableUICop": true,
    "appSourceCopSupportedCountries": "de",
    "bcContainerHelperVersion": "preview",
    "vaultNameForLocal": "BuildVariables",
    "versions": [
        {
            "version": "ci",
            "artifact": "///de/Current",
            "cacheImage": true,
            "CreateRuntimePackages": true
        },
        {
            "version": "current",
            "artifact": "///de/Current",
            "CreateRuntimePackages": true
        },
        {
            "version": "nextmajor",
            "artifact": "///de/NextMajor/{INSIDERSASTOKEN}"
        },
        {
            "version": "nextminor",
            "artifact": "///de/NextMinor/{INSIDERSASTOKEN}"
        }
    ]
}